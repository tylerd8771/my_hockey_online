<form class="form-horizontal">
    <fieldset>

        <!-- Form Name -->
        <legend>Register</legend>

        <!-- Text input-->
        <div class="form-group">
            <label class="col-md-4 control-label" for="gamertag">PSN Username</label>
            <div class="col-md-4">
                <input id="gamertag" name="gamertag" type="text" placeholder="" class="form-control input-md" required="required">
                <span class="help-block">Your PSN Username</span>
            </div>
        </div>

        <!-- Select Basic -->
        <div class="form-group">
            <label class="col-md-4 control-label" for="primaryposition">Primary Position</label>
            <div class="col-md-4">
                <select id="primaryposition" name="primaryposition" class="form-control">
      <option value="Center">Center</option>
      <option value="Left Wing">Left Wing</option>
      <option value="Right Wing">Right Wing</option>
      <option value="Left Defense">Left Defense</option>
      <option value="Right Defense">Right Defense</option>
      <option value="Goalie">Goalie</option>
    </select>
            </div>
        </div>

        <!-- Select Basic -->
        <div class="form-group">
            <label class="col-md-4 control-label" for="secondarypositions">Secondary Position</label>
            <div class="col-md-4">
                <select id="secondarypositions" name="secondarypositions" class="form-control">
      <option value="Center">Center</option>
      <option value="Left Wing">Left Wing</option>
      <option value="Right Wing">Right Wing</option>
      <option value="Left Defense">Left Defense</option>
      <option value="Right Defense">Right Defense</option>
      <option value="Goalie">Goalie</option>
    </select>
            </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
            <label class="col-md-4 control-label" for="username">PAVHL Username</label>
            <div class="col-md-4">
                <input id="username" name="username" type="text" placeholder="" class="form-control input-md" required="">
                <span class="help-block">Desired PAVHL Username</span>
            </div>
        </div>

        <!-- Password input-->
        <div class="form-group">
            <label class="col-md-4 control-label" for="password">Password</label>
            <div class="col-md-4">
                <input id="password" name="password" type="password" placeholder="" class="form-control input-md" required="">
                <span class="help-block">PAVHL Password</span>
            </div>
        </div>

    </fieldset>
</form>